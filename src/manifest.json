{
  "name": "Rastreamento de objetos - Correios",
  "short_name": "ROC",
  "version": "0.6.0",
  "author": "Édipo Juan",
  "description": "Consulte a situação de seus objetos. Obtemos essas informações diretamente da base dos Correios.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Abrir o Rastreador"
    }
  },
  "permissions": [
    "storage"
  ]
}
